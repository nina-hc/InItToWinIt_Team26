Node.java
------------------------------------------
public class Node {

    private int nodeID;
    private int occupantID = -1;

    public Node(int id) {
        this.nodeID = id;
    }

    public boolean isOccupied() {
        return occupantID != -1;
    }

    public void placeBuilding(Player player) {
        this.occupantID = player.hashCode();
    }
}





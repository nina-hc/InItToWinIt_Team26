Randomizer.java
------------------------------------------
import java.util.Random;

public class Randomizer {

    private Random random = new Random();

    public int rollDice() {
        return random.nextInt(6) + 1 + random.nextInt(6) + 1;
    }

    public int randomSelection(int min, int max) {
        return random.nextInt(max - min + 1) + min;
    }
}



------------------------------------------
Bank.java
------------------------------------------
public class Bank {

    private int grain;
    private int lumber;
    private int wool;
    private int brick;
    private int ore;

    public Bank(int grain, int lumber, int wool, int brick, int ore) {
        this.grain = grain;
        this.lumber = lumber;
        this.wool = wool;
        this.brick = brick;
        this.ore = ore;
    }

    public boolean hasResources(int lumber, int wool, int grain, int brick, int ore) {
        return this.lumber >= lumber &&
               this.wool >= wool &&
               this.grain >= grain &&
               this.brick >= brick &&
               this.ore >= ore;
    }

    public void transferToPlayer(Player player, int lumber, int wool, int grain, int brick, int ore) {
        if (hasResources(lumber, wool, grain, brick, ore)) {
            this.lumber -= lumber;
            this.wool -= wool;
            this.grain -= grain;
            this.brick -= brick;
            this.ore -= ore;
            player.getResourceHand().addResources(lumber, wool, grain, brick, ore);
        }
    }

    public void receiveFromPlayer(Player player, int lumber, int wool, int grain, int brick, int ore) {
        player.getResourceHand().removeResource(lumber, wool, grain, brick, ore);
        this.lumber += lumber;
        this.wool += wool;
        this.grain += grain;
        this.brick += brick;
        this.ore += ore;
    }
}



------------------------------------------
ResourceHand.java
------------------------------------------
public class ResourceHand {

    private int lumber;
    private int wool;
    private int grain;
    private int brick;
    private int ore;
    private int roads;
    private int settlements;
    private int cities;

    public void addResources(int lumber, int wool, int grain, int brick, int ore) {
        this.lumber += lumber;
        this.wool += wool;
        this.grain += grain;
        this.brick += brick;
        this.ore += ore;
    }

    public void removeResource(int lumber, int wool, int grain, int brick, int ore) {
        this.lumber -= lumber;
        this.wool -= wool;
        this.grain -= grain;
        this.brick -= brick;
        this.ore -= ore;
    }

    public int getTotalResources() {
        return lumber + wool + grain + brick + ore;
    }
}




public class Tile {

    private int tileID;
    private int tileRollNum;
    private String resourceType;

    public Tile(int id, int roll, String resource) {
        this.tileID = id;
        this.tileRollNum = roll;
        this.resourceType = resource;
    }

    public int getTileRollNum() {
        return tileRollNum;
    }

    public String getResourceType() {
        return resourceType;
    }
}



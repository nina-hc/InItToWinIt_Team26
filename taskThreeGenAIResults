Board.java
------------------------------------------
import java.util.List;

public class Board {

    private List<Tile> tiles;
    private List<Node> nodes;

    public Board(List<Tile> tiles, List<Node> nodes) {
        this.tiles = tiles;
        this.nodes = nodes;
    }

    public Tile getTile(int id) {
        return tiles.get(id);
    }

    public Node getNode(int id) {
        return nodes.get(id);
    }
}



